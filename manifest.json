{
    "manifest_version": 2,
    "name": "MuteTube",
    "version": "1.0",

    "description": "Automatically mutes video on every channel's homepage",
    
    "icons": { "128": "mutetube.png" },
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["test.js"],
        "persistent": false
    }
}
